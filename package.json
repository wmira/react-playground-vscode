{
  "name": "react-playground-vscode",
  "displayName": "React Playground",
  "description": "React Components Playground and Live Preview",
  "version": "0.0.1",  
  "publisher": "wmira",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openPlayground"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openPlayground",
        "title": "React Playground"
      }
    ]
  },
  "keybindings": [
    {
      "command": "_webview.openDevTools",
      "key": "ctrl+shift+b"
    }
  ],
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
